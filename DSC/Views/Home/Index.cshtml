            <div class="card">
                <div class="card-header align-items-center">
                    <h5 class="mb-0">Student Applied</h5>
                </div>

                <div class="table-responsive text-nowrap">
                    <table class="table table-hover">
                        <caption class="ms-4">
                            List of Student Applied
                        </caption>
                        <thead>
                            <tr>
                                <th>#</th>
                                <th>Student Name</th>
                                <th>Service </th>
                                <th>Date </th>
                            </tr>
                        </thead>
                        <tbody>
                            @{
                                int count = 1;
                            }
                            @foreach (var apply in Model.Applies)
                            {
                                <tr>
                                    <td>@count</td>
                                    <td>@apply.Student.User.Name</td>
                                    <td>@apply.Service.Name</td>
                                    <td>@apply.CDate.ToString("yyyy-MM-dd") </td>
                                </tr>
                                count++;

                            }
                        </tbody>
                    </table>
                </div>
            </div>
